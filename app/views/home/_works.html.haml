%section#works.section
  .container.clearfix
    %h4 Our Works
    / portfolio filter
    .row
      #filters.span12
        %ul.clearfix
          %li
            %a.active{"data-filter" => "*", :href => "#"}
              %h5 All
          %li
            %a{"data-filter" => ".web", :href => "#"}
              %h5 Web
          %li
            %a{"data-filter" => ".print", :href => "#"}
              %h5 Print
          %li
            %a{"data-filter" => ".design", :href => "#"}
              %h5 Design
          %li
            %a{"data-filter" => ".photography", :href => "#"}
              %h5 Photography
      / END PORTFOLIO FILTERING
    .row
      .span12
        #portfolio-wrap
          / portfolio item
          .portfolio-item.grid.print.photography
            .portfolio
              %a.portfolio-image{"data-pretty" => "prettyPhoto[gallery1]", :href => "img/works/big.jpg"}
                %img{:alt => "", :src => asset_url("works/1.png") }/
                .portfolio-overlay
                  .thumb-info
                    %h5 Portfolio name
                    %i.icon-plus.icon-2x
          / end portfolio item
